{
    "name": "music-generation-frontend",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite --open",
        "build": "tsc && vite build",
        "build:gh-pages": "tsc && vite build --base=/music_generation/",
        "preview": "vite preview",
        "lint": "prettier --check . --ignore-path .gitignore && eslint ./src --ext .jsx,.js,.ts,.tsx --ignore-path .gitignore",
        "lint:fix": "prettier --write . --ignore-path .gitignore && eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path .gitignore",
        "test": "vitest"
    },
    "dependencies": {
        "clsx": "^1.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.3",
        "ts-routes": "^2.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^18.0.22",
        "@types/react-dom": "^18.0.7",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@vitejs/plugin-react": "^2.2.0",
        "@vitest/ui": "^0.24.5",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "isomorphic-fetch": "^3.0.0",
        "jsdom": "^20.0.2",
        "postcss": "^8.4.18",
        "prettier": "^2.7.1",
        "prettier-plugin-tailwindcss": "^0.1.13",
        "tailwindcss": "^3.2.2",
        "typescript": "^4.6.4",
        "vite": "^3.2.0",
        "vitest": "^0.24.5"
    }
}
